#ifndef EFFECT_IMAGEEFFECT_H_
#define EFFECT_IMAGEEFFECT_H_

#include <opencv/opencv.hpp>

class ImageEffect {
public:
	virtual cv::Mat process(cv::Mat in) = 0;
	ImageEffect();
	virtual ~ImageEffect();
};

#endif /* EFFECT_IMAGEEFFECT_H_ */
